@model List<EnFu.Models.BibleReadingPdfViewModel>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<div id="bibleContentContainer" class="bibleSection">
    <div id="bibleContent">
        @Model.ForEach((item) =>
        {
            if (item == Model[0])
            {
                @<div style="margin:15px;font-weight:600;" id="Chaptor+@item.ChapterSN+_+">创世记 1</div>
            };
            @<p id="Chaptor+@item.ChapterSN+@item.VerseSN" style="border: 1px solid gray; margin:5px auto;padding:10px;">
                @item.ChapterSN @item.VerseSN @item.Lection @item.LectionEnglish
             </p>
        })
    </div>
</div>
